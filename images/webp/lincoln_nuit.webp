<svg width="400" height="250" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="250" fill="#0a0a0a"/>
  <!-- Fond nocturne avec étoiles -->
  <circle cx="50" cy="30" r="1" fill="#fff" opacity="0.8"/>
  <circle cx="150" cy="25" r="1" fill="#fff" opacity="0.6"/>
  <circle cx="280" cy="35" r="1" fill="#fff" opacity="0.9"/>
  <circle cx="350" cy="20" r="1" fill="#fff" opacity="0.7"/>

  <!-- Limousine Lincoln de nuit -->
  <rect x="50" y="90" width="300" height="100" rx="15" fill="#1a1a1a" stroke="#444" stroke-width="2"/>
  <circle cx="80" cy="210" r="15" fill="#333"/>
  <circle cx="120" cy="210" r="15" fill="#333"/>
  <circle cx="220" cy="210" r="15" fill="#333"/>
  <circle cx="280" cy="210" r="15" fill="#333"/>
  <circle cx="320" cy="210" r="15" fill="#333"/>

  <!-- Effets lumineux -->
  <rect x="100" y="110" width="200" height="50" rx="8" fill="#000" opacity="0.3"/>
  <rect x="120" y="130" width="160" height="20" rx="3" fill="#001122" opacity="0.5"/>

  <!-- Texte -->
  <text x="200" y="145" font-family="Arial, sans-serif" font-size="14" fill="#fff" text-anchor="middle">AMBIANCE</text>
  <text x="200" y="165" font-family="Arial, sans-serif" font-size="20" fill="#4169e1" text-anchor="middle" font-weight="bold">NOCTURNE</text>
  <text x="200" y="185" font-family="Arial, sans-serif" font-size="12" fill="#ccc" text-anchor="middle">Élégance sous les lumières</text>
  <text x="200" y="200" font-family="Arial, sans-serif" font-size="10" fill="#999" text-anchor="middle">de Paris</text>
</svg>